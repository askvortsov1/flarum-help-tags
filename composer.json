{
  "name": "askvortsov/flarum-private-tags",
  "description": "Allow members to post in a tag, but only allow users to view/reply to tags they have access to.",
  "keywords": [
    "flarum"
  ],
  "type": "flarum-extension",
  "license": "MIT",
  "require": {
    "flarum/core": "^0.1.0-beta.8"
  },
  "authors": [
    {
      "name": "Alexander Skvortsov",
      "email": "askvortsov@youthneuro.org",
      "role": "Developer"
    }
  ],
  "autoload": {
    "psr-4": {
      "Askvortsov\\FlarumPrivateTags\\": "src/"
    }
  },
  "extra": {
    "flarum-extension": {
      "title": "Flarum Private Tags",
      "icon": {
        "name": "",
        "backgroundColor": "",
        "color": ""
      }
    },
    "flagrow": {
      "discuss": ""
    }
  }
}
